@charset "utf-8";
/* CSS Document */
<style type="text/css">
body {
	font: 100% Verdana, Arial, Helvetica, sans-serif;
	margin: 0 auto; /* 最好将 body 元素的边距和填充设置为 0 以覆盖不同的浏览器默认值 */
	padding: 0;
}
.container {
	width:980px;
	height:auto;
	margin:0 auto;
	padding:0;
	position:relative;
}
.header {
	width:980px;
	height:120px;
	position:relative;
	margin-bottom:10px;
	background:url('../img/pbcls.jpg') 0px 0px no-repeat;  /*增加了“..”作为路径导向-06-03wmc  */
}

.maincontent {
	width:980px;
	height:auto;
}

.navbar {
	text-align:center;
	width:200px;
	float:left;
	clear:left;
	overflow:hidden;
	margin-left:10px;
	height:auto;
	position:relative;
}

.self_info{
	text-align:center;
	margin:0 5px 0 5px;
	width:190px;
}

.nav {
	text-align:left;
	margin:5px 5px 0 5px;
	width:190px;
	height:100px;
	list-style-type:none;
}
.nav li a {
	border-top:#E7E7E7 solid;
	color:#000000;
	text-decoration:none;
	padding:4px auto 4px;
	display:block;
	width:90px;
	height:18px;
	text-align:center;
	margin-left:2px;
	font-size:14px;
	font-family:"宋体", "隶书", "楷体";
	list-style-type:none;
}

.nav li a:hover {
	background-color:#94CBF3;
	color:#FFFFFF;
}

.team {
	text-align:center;
	margin:5px 5px 0 5px;
	width:190px;
	height:100px;
}

.newsbar {
	text-align:center;
	width:200px;
	float:right;
	clear:right;
	margin-right:10px;
	overflow:hidden;
	height:auto;
	position:relative;
}

.pro_news {
	width:190px;
	margin:0 5px 0 5px;
	height:200px;
	border:solid #E7E7E7;
	border-width:1px;
}

.im_news {
	width:190px;
	margin-top:5px;
	margin-bottom:20px;
	height:300px;
	border:solid #E7E7E7;
	border-width:1px;
}

.ins_out {
	text-align:left;
	width:190px;
	margin:5px 5px 0 5px;
	border:solid #E7E7E7;
	border-width:1px;
	vertical-align:middle;
}

.ins_out form{
	font-size:12px;
	vertical-align:top;
}

.ins_out select {
        position:relative;
        left:-2px;
        top:-2px;
        font-size:12px;
        width:105px;
        line-height:18px;border:0px;
        color:#909993;
}

.category {
	text-align:left;
	margin-left:10px;
	width:500px;
	height:25px;
	padding-left:2px;
	list-style-type:none;
	position:relative;
}

.category li{
	float:left;
}

.category li a{
	color:#000000;
	text-decoration:none;
	padding:2px auto 2px;
	display:block;
	width:97px;
	height:18px;
	text-align:center;
	margin-left:2px;
}

.category li a:hover {
	background-color:#94CBF3;
	color:#FFFFFF;
}

.screen {
	margin:0px auto;
	overflow:hidden;
	width:500px;
	height:400px;
	border:2px #F2F2F2 solid;
	position:relative;
	padding:0 auto;
}

.main {
	text-align:left;
	width:540px;
	margin:5px auto;
	height:auto;
	padding-left:10px;
	padding-top:10px;
}

.main textarea{
	text-align:left;
}

.main p {
	font-size:12px;
	margin:0 auto;
}

.main span{
	font-size:12px;
}

.mainbody {
	text-align:center;
	padding-top:0px;
	margin:0 auto;
	width:550px;
	height:auto;
}

.game_nav{
	text-align:left;
	margin:5px auto;
	width:540px;
	height:25px;
	padding-left:2px;
	font-size:14px;
}


.footer {
	clear:both;
	width:980px;
	height:50px;
}
</style>